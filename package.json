{
  "name": "music-streamer",
  "version": "1.0.0",
  "description": "Server that gets music from yt or spotify via youtube-dl and then streams it via sockets",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register build/index.js",
    "dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:unit": "npm run test -- /Domain /Application",
    "test:watch": "npm run test -- --watch"
  },
  "author": "martimalek",
  "license": "ISC",
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^0.9.1",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.23",
    "@types/mongodb": "^3.6.17",
    "@types/node": "^15.0.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.14",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-import-node": "^0.0.4",
    "graphql-tools": "^7.0.5",
    "mongodb": "^3.6.9",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2",
    "youtube-dl-exec": "^1.2.3"
  }
}
